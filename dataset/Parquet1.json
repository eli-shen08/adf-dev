{
	"name": "Parquet1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ADLS_Conn",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schemaname": {
				"type": "string"
			},
			"tablename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().tablename,'.parquet')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('/bronze',dataset().schemaname,'/',dataset().tablename)",
					"type": "Expression"
				},
				"fileSystem": "retail"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}